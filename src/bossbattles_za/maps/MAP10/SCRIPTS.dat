Script 299 (void) // Untrustables Monsters counter - Experimental. Normally Skulltag does the monster counting in invasion mode...
{
SetFont("BIGFONT");
HudMessageBold(s: "Monsters left: ", d:ThingCount(0, 999);
                HUDMSG_PLAIN, 0, CR_Red, -0.0, -0.75, 1.0, 0.03, 0);
If ( (ThingCount(0, 999)) == 0 )
  {
  SetFont("BIGFONT");
  HudMessage(s:"Well Done...!"; HUDMSG_PLAIN, 1, CR_RED, 0.5, 0.5, 2.5, 0.05, 1.0);
  Ambientsound("dswelld",255);
  Delay(10);
  Wave = Wave + 1;
  WellDone = 0;
  Delay(10);
  Acs_Terminate (299, 0);
  }
Delay(const:35);
Restart;


}

