     Script 1 Enter
{
if (GameType () == GAME_SINGLE_PLAYER)
{
            ACS_Execute(2, 0, 0, 0, 0); // Invasion Script Single Player
}
Else
        ACS_Execute(3, 0, 0, 0, 0); // Invasion Script Multiplayer
}

